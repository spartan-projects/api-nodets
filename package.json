{
  "name": "api-nodets",
  "version": "1.0.0",
  "description": "Typescript port from the api-nodejs project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "commit": "cz",
    "clean": "rm -rf dist/",
    "build": "tsc",
    "start": "node dist/index.js",
    "start-dev": "nodemon dist/index.js",
    "prettier": "npx prettier --write .",
    "eslint": "npx eslint .",
    "prepare": "npm run prettier && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartan-projects/api-nodets.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spartan-projects/api-nodets/issues"
  },
  "homepage": "https://github.com/spartan-projects/api-nodets#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
